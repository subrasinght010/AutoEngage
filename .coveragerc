[run]
omit =
    */tests/*
    */venv/*
    */.venv/*
    */env/*
    setup.py
    */site-packages/*
    */migrations/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod